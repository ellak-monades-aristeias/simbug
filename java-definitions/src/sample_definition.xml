<definition>
<configuration>
	<parameter>
		<name>config1</name>
		<type>Integer</type>
		<value>4</value>
		<comment>The first configuration parameter</comment>
	</parameter>
	
	<parameter>
		<name>config2</name>
		<type>Integer</type>
		<value>5</value>
		<comment>The second configuration parameter</comment>
	</parameter>
</configuration>


<random_number_generators>
	<random_number_generator>
		<name>uniform</name>
		<expression>return Math.random();</expression> 
	</random_number_generator>
</random_number_generators>


<variables>
	<player_choices>
		<variable>
			<name>choice1</name>
			<type>Integer</type>
		</variable>
		
		<variable>
			<name>choice2</name>
			<type>Integer</type>
		</variable>
	</player_choices>
	
	<user_state>
		<variable>
			<name>userState1</name>
			<type>Integer</type>
		</variable>
		
		<variable>
			<name>userState2</name>
			<type>Integer</type>
		</variable>
	</user_state>
	
	<world_state>
		<variable>
			<name>worldState1</name>
			<type>Integer</type>
		</variable>
	</world_state>
	
	<external>
	</external>
</variables>


<choices_to_state_algorithm>
	<players>
		userState1 = AVERAGE(Choice1) + Choice2;
		userState2 = Choice1 + AVERAGE(Choice2);	
	</players>
	<world>
		worldState1 = UserState1 - UserState2;	
	</world>
</choices_to_state_algorithm>
</definition>
